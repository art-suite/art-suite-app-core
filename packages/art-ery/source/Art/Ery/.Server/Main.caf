import &ArtStandardLib, &ArtCommunicationStatus, &Art/Ery
&colors

start: (options) =>
  config.location = :server
  &ArtExpressServer.start
    options
    verbose: config.verbose
    commonResponseHeaders: merge
      &ArtExpressServer.AllowAllCorsHandler.commonResponseHeaders
      options.commonResponseHeaders

    handlers: compactFlatten []
      &ArtExpressServer.AllowAllCorsHandler
      &ArtEryHandler
      &ArtEryInfoHandler
      options.handlers
