{"version":3,"file":"","sourceRoot":"","sources":["./source/Art.SvgPathExtractor/EncodedPath.caf"],"sourceContent":["import &StandardImport\n\nclass EncodedPath extends BaseClass\n\n  constructor: (props) ->\n    @props = object v, k in props\n      if isNumber v\n        numberToTightString v, 5\n      else\n        v\n\n    @points = @props.points\n    @radii = @props.radii\n    @commands = @props.commands\n\n    @reset()\n\n  @getter\n    inspectedObjects: -> @props\n\n    xbd: -> PathTag @props\n\n    drawArea: ->\n      @reset()\n      maxX = minX = maxY = minY = 0\n      while p = @nextPoint\n        {x, y} = p\n        maxX = max maxX, x\n        maxY = max maxY, y\n        minX = min minX, x\n        minY = min minY, y\n\n      rect\n        minX\n        minY\n        maxX - minX\n        maxY - minY\n\n    nextPoint: ->\n      {points, _scaleY, _scaleX, _addX, _addY} = @\n      if @_pointIndex < points.length\n        x: points[@_pointIndex++] * _scaleX + _addX\n        y: points[@_pointIndex++] * _scaleY + _addY\n\n  reset: ->\n    @_pointIndex =\n    @_radiiIndex =\n    @_commandIndex = 0\n\n    @props extract x = 0, y = 0, aspectRatio = 1, scale = 1\n    @_scaleX =\n    @_scaleY = scale\n    if aspectRatio > 1\n      @_scaleY /= aspectRatio\n    else\n      @_scaleX *= aspectRatio\n\n    @_addX = @props.x ? 0\n    @_addY = @props.y ? 0\n\n"],"names":[],"mappings":"AAAA;;8IAAO,uHAEP,wCAAM,oBAAoB,sBAEX,CAAC,6BACZ,wBAAwB,OAAR,IAAG,MACjB,AAAG,SAAS,KAAC,AACX,oBAAoB,GAAG,KACrB,AACF,IAEJ,cAAU,WAAO,QACjB,aAAS,WAAO,OAChB,gBAAY,WAAO,UAEnB,oEAEF,YAAO,CACL,kBAAkB,YAAG,qBAErB,KAAK,YAAG,eAAQ,eAEhB,UAAU,iDAAE,AACV,cACA,OAAO,OAAO,OAAO,OAAO,GAC5B,OAAM,IAAI,iBAAU,AAClB,EAAC,GAAG,KAAK,IACT,OAAO,IAAI,MAAM,IACjB,OAAO,IAAI,MAAM,IACjB,OAAO,IAAI,MAAM,IACjB,OAAO,IAAI,MAAM,aAEnB,KACE,MACA,MACA,OAAO,MACP,OAAO,SAEX,WAAW,wDAAE,AACX,EAAC,QAAQ,SAAS,SAAS,OAAO,SAAS,cAC3C,AAAG,oBAAe,OAAO,UAAM,CAC7B,GAAG,AAAsB,AAAtB,OAAO,sBAAkB,UAAU,OACtC,GAAG,AAAsB,AAAtB,OAAO,sBAAkB,UAAU,+CAErC,0FAAE,AA