import &ArtSuite

class PlanetMonitor extends Component
  render: ->
    Element
      size: ww: 1 hch: 1
      margin: &StyleProps.basePadding
      RectangleElement
        &StyleProps.planetMonitorBackground
        FillElement()
        OutlineElement &StyleProps.planetMonitorOutline

      TextElement
        &StyleProps.planetMonitorText
        text: @props.location?.name || "unknown"
